// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id            String       @default(cuid()) @id
  name          String
  email         String       @unique
  phoneNumber   String
  password      String
  @@map(name: "users")
}